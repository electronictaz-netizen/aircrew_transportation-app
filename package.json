{
  "name": "aircrew-transportation-app",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "check-lambda": "npx ts-node --esm scripts/checkLambdaRuntimes.ts",
    "check-lambda:update": "npx ts-node --esm scripts/checkLambdaRuntimes.ts --update",
    "check-deps": "npx ts-node --esm scripts/updateToLatest.ts",
    "update-all": "npx ts-node --esm scripts/updateToLatest.ts",
    "update-all:apply": "npx ts-node --esm scripts/updateToLatest.ts --apply"
  },
  "dependencies": {
    "@aws-amplify/backend": "^1.20.0",
    "@aws-amplify/backend-cli": "^1.8.2",
    "@aws-amplify/ui-react": "^6.0.0",
    "aws-amplify": "^6.0.0",
    "aws-cdk-lib": "^2.140.0",
    "axios": "^1.7.9",
    "constructs": "^10.4.2",
    "date-fns": "^4.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@aws-sdk/client-lambda": "^3.971.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/xlsx": "^0.0.36",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.18.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "sharp": "^0.33.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vite": "^7.3.1",
    "vite-plugin-pwa": "^1.2.0"
  }
}
